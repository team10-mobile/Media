<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:angle="270"
                android:endColor="#FCFBFB"
                android:startColor="#A87A7A" />
            <stroke
                android:width="1dp"
                android:color="#807E7E" />
            <corners
                android:radius="25dp" />
